<!DOCTYPE html>
<html>
  <head>
    <title>QaThinknetikaApp</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= Gon::Base.render_data %>
  </head>
<%#console%>
  <body>
  <div id="msg"></div>
  <%= link_to "Questions index", root_path %>
  <%= link_to "Questions new", new_question_path %>
  <%= link_to "My profile", user_path(current_user) if user_signed_in? %>
  <%= link_to "Log in", new_user_session_path unless user_signed_in? %>
  <%= link_to "Log out", destroy_user_session_path, method: :delete if user_signed_in? %>
  <hr>
  <% if notice %>
    <p class="alert alert-success"><%= notice %></p>
  <% end %>
  <% if alert %>
    <p class="alert alert-danger"><%= alert %></p>
  <% end %>
    <%= yield %>
  </body>
</html>